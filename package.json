{
  "name": "@jeswr/cookie-analysis",
  "version": "0.0.0-development",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "test": "jest",
    "lint": "eslint lib/*.ts __tests__/* --ext .ts",
    "lint:fix": "eslint lib/*.ts __tests__/* --ext .ts --fix",
    "build": "tsc",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "start": "ts-node lib/index.ts",
    "dtou": "ts-node lib/dtou.ts",
    "odrl": "ts-node lib/odrl.ts",
    "secret:set": "gh secret set -f .env",
    "generate:shapes": "ts-node lib/odrlShapeGen2.ts",
    "generate:shex": "shacl2shex shapes/ .shex/",
    "generate:ldo": "ldo build -i shapes/ -o lib/ldo/",
    "generate": "npm run generate:shapes && npm run generate:shex && npm run generate:ldo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeswr/cookie-analysis"
  },
  "keywords": [],
  "author": "Jesse Wright <https://github.com/jeswr/>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeswr/cookie-analysis/issues"
  },
  "homepage": "https://github.com/jeswr/cookie-analysis/README.md",
  "devDependencies": {
    "@rdfjs/types": "^2.0.1",
    "@types/jest": "^30.0.0",
    "@types/make-fetch-happen": "^10.0.4",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "pre-commit": "^1.2.2",
    "semantic-release": "^25.0.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "pre-commit": [
    "lint",
    "build",
    "test"
  ],
  "release": {
    "branches": [
      "main",
      "+([0-9])?(.{+([0-9]),x}).x",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@jeswr/rdf-serialize-store": "^1.1.1",
    "@jeswr/shacl2shex": "^1.5.1",
    "@jeswr/utils": "^1.1.0",
    "@langchain/community": "^1.0.6",
    "@langchain/core": "^1.0.6",
    "@langchain/openai": "^1.1.3",
    "@ldo/cli": "^0.0.1-1.0.0-alpha.1.0",
    "@ldo/ldo": "^0.0.1-1.0.0-alpha.1.0",
    "@types/n3": "^1.26.1",
    "dotenv": "^17.2.3",
    "dumb-csv": "^1.1.2",
    "eyereasoner": "^18.23.0",
    "make-fetch-happen": "^15.0.3",
    "langchain": "^1.1.2",
    "md5": "^2.3.0",
    "n3": "^1.26.0",
    "node-html-parser": "^7.0.1",
    "rdf-dereference-store": "^1.4.0",
    "rdf-namespaces": "^1.16.0"
  }
}
