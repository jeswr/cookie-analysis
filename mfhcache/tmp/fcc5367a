    <dct:title xml:lang="en">Private Relationships in Social Networks</dct:title>
    <dct:creator rdf:resource="#carminatib"/>
    <dct:creator rdf:resource="#ferrarie"/>
    <dct:creator rdf:resource="#me"/>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#gYearMonth">2007-4</dct:issued>
