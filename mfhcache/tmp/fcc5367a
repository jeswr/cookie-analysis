    <dct:title xml:lang="en">The Quality Social Network: A Collaborative Environment for Personalizing Web Access</dct:title>
    <dct:creator rdf:resource="#me"/>
    <dct:creator rdf:resource="#carminatib"/>
    <dct:creator rdf:resource="#ferrarie"/>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#gYearMonth">2009-11</dct:issued>
    <dctype:type rdf:resource="http://purl.org/dc/dcmitype/Text"/>
    <dct:isPartOf>
      <rdf:Description rdf:about="#collaboratesn">
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#Proceedings"/>
        <dct:title>International Workshop on Collaborative Social Networks</dct:title>
