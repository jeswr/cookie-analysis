        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#Journal"/>
        <dct:title>IEEE Transactions on Knowledge &amp; Data Engineering</dct:title>
        <bibo:volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">26</bibo:volume>
