        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#Journal"/>
        <dct:title>Data &amp; Knowledge Engineering</dct:title>
        <bibo:volume rdf:datatype="http://www.w3.org/2001/XMLSchema#string">79-80</bibo:volume>
